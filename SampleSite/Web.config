    <Controls>
      <Control Type="Gizmox.WebGUI.Forms.Editors.*, IKGD_Author_CKEditor, Culture=neutral" />
      <Control Type="Gizmox.WebGUI.Forms.*, Gizmox.WebGUI.Forms.Extended, Culture=neutral" />
