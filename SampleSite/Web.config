      <Application Code="Default" Type="Ikon.IKGD.IKGD_Main, 'IKGD_Author_Engine_v1'" Authentication="" />
      <Application Code="Author" Type="Ikon.IKGD.IKGD_Main, 'IKGD_Author_Engine_v1'" Authentication="" />
